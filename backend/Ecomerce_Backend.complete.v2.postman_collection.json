        { "name": "Get Promotion By Id", "request": { "method": "GET", "header": [], "url": { "raw": "{{baseUrl}}/promociones/:id", "host": ["{{baseUrl}}"], "path": ["promociones",":id"] }, "description": "Reemplaza :id" } },
