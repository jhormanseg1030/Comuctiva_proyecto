        { "name": "Get Subcategory By Id", "request": { "method": "GET", "header": [], "url": { "raw": "{{baseUrl}}/subcategorias/:id", "host": ["{{baseUrl}}"], "path": ["subcategorias",":id"] }, "description": "Reemplaza :id" } },
