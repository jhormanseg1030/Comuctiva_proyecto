{
  "info": {
    "name": "Ecomerce Backend - Complete Collection v2",
    "_postman_id": "ecomerce-backend-complete-2025-v2",
    "description": "Versi贸n 2: Colecci贸n completa para probar el backend Ecomerce (login, usuarios, productos, categorias, carrito, pedidos, comentarios). Variables: baseUrl, authToken.",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    { "key": "baseUrl", "value": "http://localhost:8080/api" },
    { "key": "authToken", "value": "" }
  ],
  "item": [
    {
      "name": "Auth",
      "item": [
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "header": [ { "key": "Content-Type", "value": "application/json" } ],
            "body": { "mode": "raw", "raw": "{\n  \"numeroDocumento\": \"12345678\",\n  \"password\": \"password123\"\n}" },
            "url": { "raw": "{{baseUrl}}/auth/login", "host": ["{{baseUrl}}"], "path": ["auth","login"] }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "  var json = pm.response.json();",
                  "  if (json && json.token) { pm.environment.set('authToken', json.token); pm.test('Token guardado', function(){pm.expect(json.token).to.exist;}); }",
                  "} else { pm.test('Login OK', function(){pm.expect(pm.response.code).to.be.oneOf([200,201]); }); }"
                ]
              }
            }
          ]
        },
        {
          "name": "Register",
          "request": {
            "method": "POST",
            "header": [ { "key": "Content-Type", "value": "application/json" } ],
            "body": { "mode": "raw", "raw": "{\n  \"numeroDocumento\": \"12345678\",\n  \"tipoDocumento\": \"CC\",\n  \"password\": \"password123\",\n  \"nombre\": \"Juan\",\n  \"apellido\": \"Perez\",\n  \"telefono\": \"3001234567\",\n  \"direccion\": \"Calle 123\",\n  \"correo\": \"juan@example.com\",\n  \"rol\": \"USER\"\n}" },
            "url": { "raw": "{{baseUrl}}/auth/register", "host": ["{{baseUrl}}"], "path": ["auth","register"] }
          }
        }
      ]
    },
    {
      "name": "Usuarios",
      "item": [
        { "name": "List All Users (admin)", "request": { "method": "GET", "header": [ { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "url": { "raw": "{{baseUrl}}/usuarios", "host": ["{{baseUrl}}"], "path": ["usuarios"] } } },
        { "name": "Get User By Id", "request": { "method": "GET", "header": [ { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "url": { "raw": "{{baseUrl}}/usuarios/:numeroDocumento", "host": ["{{baseUrl}}"], "path": ["usuarios", ":numeroDocumento"] }, "description": "Reemplazar :numeroDocumento por el documento a consultar" } },
        { "name": "Create User (register)", "request": { "method": "POST", "header": [ { "key": "Content-Type", "value": "application/json" } ], "body": { "mode": "raw", "raw": "{\n  \"numeroDocumento\": \"99999999\",\n  \"tipoDocumento\": \"CC\",\n  \"password\": \"pass999\",\n  \"nombre\": \"Prueba\",\n  \"apellido\": \"User\",\n  \"telefono\": \"3000000000\",\n  \"direccion\": \"Calle Prueba\",\n  \"correo\": \"prueba@example.com\",\n  \"rol\": \"USER\"\n}" }, "url": { "raw": "{{baseUrl}}/auth/register", "host": ["{{baseUrl}}"], "path": ["auth","register"] } } },
        { "name": "Update User (JSON)", "request": { "method": "PUT", "header": [ { "key": "Content-Type", "value": "application/json" }, { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "body": { "mode": "raw", "raw": "{\n  \"tipoDocumento\": \"CC\",\n  \"password\": \"password123\",\n  \"nombre\": \"Juanito\",\n  \"apellido\": \"Perez\",\n  \"telefono\": \"3000000000\",\n  \"direccion\": \"Nueva calle 1\",\n  \"correo\": \"juanito@example.com\",\n  \"rol\": \"USER\",\n  \"activo\": true\n}" }, "url": { "raw": "{{baseUrl}}/usuarios/:numeroDocumento", "host": ["{{baseUrl}}"], "path": ["usuarios", ":numeroDocumento"] }, "description": "Reemplaza :numeroDocumento" } },
        { "name": "Change Role (admin)", "request": { "method": "PUT", "header": [ { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "url": { "raw": "{{baseUrl}}/usuarios/:numeroDocumento/rol?rol=ADMIN", "host": ["{{baseUrl}}"], "path": ["usuarios",":numeroDocumento","rol"], "query": [{"key":"rol","value":"ADMIN"}] }, "description": "Reemplaza :numeroDocumento" } },
        { "name": "Change Estado (admin)", "request": { "method": "PUT", "header": [ { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "url": { "raw": "{{baseUrl}}/usuarios/:numeroDocumento/estado?activo=false", "host": ["{{baseUrl}}"], "path": ["usuarios",":numeroDocumento","estado"], "query": [{"key":"activo","value":"false"}] }, "description": "Reemplaza :numeroDocumento" } },
        { "name": "Delete User (admin)", "request": { "method": "DELETE", "header": [ { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "url": { "raw": "{{baseUrl}}/usuarios/:numeroDocumento", "host": ["{{baseUrl}}"], "path": ["usuarios", ":numeroDocumento"] }, "description": "Reemplaza :numeroDocumento" } },
        { "name": "Change Password", "request": { "method": "PUT", "header": [ { "key": "Content-Type", "value": "application/json" }, { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "body": { "mode": "raw", "raw": "{\n  \"passwordActual\": \"password123\",\n  \"passwordNueva\": \"nuevoPassword123\"\n}" }, "url": { "raw": "{{baseUrl}}/usuarios/:numeroDocumento/cambiar-password", "host": ["{{baseUrl}}"], "path": ["usuarios", ":numeroDocumento","cambiar-password"] }, "description": "Reemplaza :numeroDocumento" } }
      ]
    },
    {
      "name": "Productos",
      "item": [
        { "name": "List All", "request": { "method": "GET", "header": [], "url": { "raw": "{{baseUrl}}/productos", "host": ["{{baseUrl}}"], "path": ["productos"] } } },
        { "name": "List Activos", "request": { "method": "GET", "header": [], "url": { "raw": "{{baseUrl}}/productos/activos", "host": ["{{baseUrl}}"], "path": ["productos","activos"] } } },
        { "name": "Get By Id", "request": { "method": "GET", "header": [], "url": { "raw": "{{baseUrl}}/productos/:id", "host": ["{{baseUrl}}"], "path": ["productos",":id"] }, "description": "Reemplaza :id" } },
        { "name": "Create (JSON)", "request": { "method": "POST", "header": [ { "key": "Content-Type", "value": "application/json" }, { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "body": { "mode": "raw", "raw": "{\n  \"nombre\": \"Tomate\",\n  \"descripcion\": \"Tomate rojo fresco\",\n  \"precio\": 1200.50,\n  \"stock\": 50,\n  \"categoria\": { \"id\": 1 },\n  \"subcategoria\": { \"id\": 1 },\n  \"usuario\": { \"numeroDocumento\": \"12345678\" }\n}" }, "url": { "raw": "{{baseUrl}}/productos", "host": ["{{baseUrl}}"], "path": ["productos"] } } },
        { "name": "Create with Image (form-data)", "request": { "method": "POST", "header": [ { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "body": { "mode": "formdata", "formdata": [ { "key": "nombre", "value": "Tomate con imagen", "type": "text" }, { "key": "descripcion", "value": "Descripci贸n ejemplo", "type": "text" }, { "key": "precio", "value": "1200.00", "type": "text" }, { "key": "stock", "value": "20", "type": "text" }, { "key": "categoriaId", "value": "1", "type": "text" }, { "key": "subcategoriaId", "value": "1", "type": "text" }, { "key": "usuarioDocumento", "value": "12345678", "type": "text" }, { "key": "imagen", "type": "file", "src": [] } ] }, "url": { "raw": "{{baseUrl}}/productos/con-imagen", "host": ["{{baseUrl}}"], "path": ["productos","con-imagen"] } } },
        { "name": "Update Product (JSON)", "request": { "method": "PUT", "header": [ { "key": "Content-Type", "value": "application/json" }, { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "body": { "mode": "raw", "raw": "{\n  \"nombre\": \"Mango Actualizado\",\n  \"descripcion\": \"Descripci贸n actualizada\",\n  \"precio\": 1300.00,\n  \"stock\": 60,\n  \"activo\": true,\n  \"categoria\": { \"id\": 1, \"nombre\": \"Verduras\" },\n  \"subcategoria\": { \"id\": 1, \"nombre\": \"Tomates\" },\n  \"usuario\": { \"numeroDocumento\": \"12345678\", \"nombre\": \"Juan\" },\n  \"imagenUrl\": null\n}" }, "url": { "raw": "{{baseUrl}}/productos/:id", "host": ["{{baseUrl}}"], "path": ["productos",":id"] }, "description": "Reemplaza :id. Incluye objetos anidados (categoria, subcategoria, usuario) porque el backend usa relaciones no inicializadas." } },
        { "name": "Activate/Deactivate Product", "request": { "method": "PUT", "header": [ { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "url": { "raw": "{{baseUrl}}/productos/:id/estado?activo=false", "host": ["{{baseUrl}}"], "path": ["productos",":id","estado"], "query": [{"key":"activo","value":"false"}] }, "description": "Reemplaza :id" } },
        { "name": "Update Stock", "request": { "method": "PUT", "header": [ { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "url": { "raw": "{{baseUrl}}/productos/:id/stock?cantidad=10", "host": ["{{baseUrl}}"], "path": ["productos",":id","stock"], "query": [{"key":"cantidad","value":"10"}] }, "description": "Reemplaza :id" } },
        { "name": "Delete Product", "request": { "method": "DELETE", "header": [ { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "url": { "raw": "{{baseUrl}}/productos/:id", "host": ["{{baseUrl}}"], "path": ["productos",":id"] }, "description": "Reemplaza :id" } }
      ]
    },
    {
      "name": "Categorias",
      "item": [
        { "name": "List All Categories", "request": { "method": "GET", "header": [], "url": { "raw": "{{baseUrl}}/categorias", "host": ["{{baseUrl}}"], "path": ["categorias"] } } },
        { "name": "Get Category By Id", "request": { "method": "GET", "header": [], "url": { "raw": "{{baseUrl}}/categorias/:id", "host": ["{{baseUrl}}"], "path": ["categorias",":id"] }, "description": "Reemplaza :id" } },
        { "name": "Create Category (admin)", "request": { "method": "POST", "header": [ { "key": "Content-Type", "value": "application/json" }, { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "body": { "mode": "raw", "raw": "{\n  \"nombre\": \"Verduras\"\n}" }, "url": { "raw": "{{baseUrl}}/categorias", "host": ["{{baseUrl}}"], "path": ["categorias"] } } },
        { "name": "Update Category (admin)", "request": { "method": "PUT", "header": [ { "key": "Content-Type", "value": "application/json" }, { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "body": { "mode": "raw", "raw": "{\n  \"nombre\": \"Frutas y verduras\"\n}" }, "url": { "raw": "{{baseUrl}}/categorias/:id", "host": ["{{baseUrl}}"], "path": ["categorias",":id"] }, "description": "Reemplaza :id" } },
        { "name": "Change Category Estado (admin)", "request": { "method": "PUT", "header": [ { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "url": { "raw": "{{baseUrl}}/categorias/:id/estado?activo=false", "host": ["{{baseUrl}}"], "path": ["categorias",":id","estado"], "query": [{"key":"activo","value":"false"}] }, "description": "Reemplaza :id" } },
        { "name": "Delete Category (admin)", "request": { "method": "DELETE", "header": [ { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "url": { "raw": "{{baseUrl}}/categorias/:id", "host": ["{{baseUrl}}"], "path": ["categorias",":id"] }, "description": "Reemplaza :id" } }
      ]
    },
    {
      "name": "Comentarios",
      "item": [
        { "name": "Create Comment", "request": { "method": "POST", "header": [ { "key": "Content-Type", "value": "application/json" }, { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "body": { "mode": "raw", "raw": "{\n  \"productoId\": 1,\n  \"comentario\": \"Buen producto\",\n  \"calificacion\": 5\n}" }, "url": { "raw": "{{baseUrl}}/comentarios", "host": ["{{baseUrl}}"], "path": ["comentarios"] } } },
        { "name": "Get By Product", "request": { "method": "GET", "header": [], "url": { "raw": "{{baseUrl}}/comentarios/producto/:productoId", "host": ["{{baseUrl}}"], "path": ["comentarios","producto",":productoId"] }, "description": "Reemplaza :productoId" } },
        { "name": "Update Comment", "request": { "method": "PUT", "header": [ { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "url": { "raw": "{{baseUrl}}/comentarios/:id?comentario=Actualizado&calificacion=4", "host": ["{{baseUrl}}"], "path": ["comentarios",":id"], "query": [{"key":"comentario","value":"Actualizado"},{"key":"calificacion","value":"4"}] }, "description": "Reemplaza :id" } },
        { "name": "Delete Comment", "request": { "method": "DELETE", "header": [ { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "url": { "raw": "{{baseUrl}}/comentarios/:id", "host": ["{{baseUrl}}"], "path": ["comentarios",":id"] }, "description": "Reemplaza :id" } }
      ]
    },
    {
      "name": "Carrito",
      "item": [
        { "name": "Get My Cart", "request": { "method": "GET", "header": [ { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "url": { "raw": "{{baseUrl}}/carrito", "host": ["{{baseUrl}}"], "path": ["carrito"] } } },
        { "name": "Add To Cart", "request": { "method": "POST", "header": [ { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "url": { "raw": "{{baseUrl}}/carrito/agregar?productoId=:productoId&cantidad=:cantidad", "host": ["{{baseUrl}}"], "path": ["carrito","agregar"], "query": [{"key":"productoId","value":":productoId"},{"key":"cantidad","value":":cantidad"}] }, "description": "Reemplaza :productoId y :cantidad" } },
        { "name": "Update Cart Item", "request": { "method": "PUT", "header": [ { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "url": { "raw": "{{baseUrl}}/carrito/:id?cantidad=5", "host": ["{{baseUrl}}"], "path": ["carrito",":id"] }, "description": "Reemplaza :id" } },
        { "name": "Delete Cart Item", "request": { "method": "DELETE", "header": [ { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "url": { "raw": "{{baseUrl}}/carrito/:id", "host": ["{{baseUrl}}"], "path": ["carrito",":id"] }, "description": "Reemplaza :id" } },
        { "name": "Clear Cart", "request": { "method": "DELETE", "header": [ { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "url": { "raw": "{{baseUrl}}/carrito/vaciar", "host": ["{{baseUrl}}"], "path": ["carrito","vaciar"] } } }
      ]
    },
    {
      "name": "Pedidos",
      "item": [
        { "name": "Create Order (from cart)", "request": { "method": "POST", "header": [ { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "url": { "raw": "{{baseUrl}}/pedidos/crear?direccionEnvio=Calle+123&metodoPago=efectivo&costoFlete=0", "host": ["{{baseUrl}}"], "path": ["pedidos","crear"], "query": [{"key":"direccionEnvio","value":"Calle 123"},{"key":"metodoPago","value":"efectivo"},{"key":"costoFlete","value":"0"}] } } },
        { "name": "Get My Orders", "request": { "method": "GET", "header": [ { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "url": { "raw": "{{baseUrl}}/pedidos/mis-pedidos", "host": ["{{baseUrl}}"], "path": ["pedidos","mis-pedidos"] } } },
        { "name": "Get My Sales (mis-ventas)", "request": { "method": "GET", "header": [ { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "url": { "raw": "{{baseUrl}}/pedidos/mis-ventas", "host": ["{{baseUrl}}"], "path": ["pedidos","mis-ventas"] } } },
        { "name": "Get All Orders (admin)", "request": { "method": "GET", "header": [ { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "url": { "raw": "{{baseUrl}}/pedidos", "host": ["{{baseUrl}}"], "path": ["pedidos"] } } },
        { "name": "Get Order Detail", "request": { "method": "GET", "header": [ { "key": "Authorization", "value": "Bearer {{authToken}}" } ], "url": { "raw": "{{baseUrl}}/pedidos/:id", "host": ["{{baseUrl}}"], "path": ["pedidos",":id"] }, "description": "Reemplaza :id" } },
